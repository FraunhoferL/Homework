}

code12
#include<iostream>
using namespace std;
int main()
{
	int a=5,b=8;
	a=a+b;
	b=a-b;
	a=a-b;
	cout<<"a="<<a<<endl; 
	cout<<"b="<<b<<endl;
	return 0;
}

code13
#include<iostream>
using namespace std;
int main()
{
	int a=5,b=9;
	a=a^b;
	b=b^a;
	a=a^b;
	cout<<"a="<<a<<endl; 
	cout<<"b="<<b<<endl;
	return 0;
}
